{
    "name": "tws/heroicons",
    "license": "MIT",
    "homepage": "https://custura.de",
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TWS\\Heroicons\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "bin": [
        "bin/heroicons"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TWS\\Heroicons\\HeroiconsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
